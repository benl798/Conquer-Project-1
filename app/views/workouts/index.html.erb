<h2>All Workouts</h2>
<ul>
  <% @workouts.each do |workout| %>
    <%# link_to workout.workout_type, workout_path(workout.id) %>
    <% if workout.image.present? %>
      <%= link_to cl_image_tag(workout.image), workout_path(workout.id), alt: workout.workout_type, class: 'workout' %>
    <% end %>
  <% end %>
</ul>
